# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'GitHub Stars' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GitHub Stars
  pod 'Alamofire', '~> 4.7' #HTTP Networking
  pod 'SVProgressHUD', '~> 2.2' #A clean and lightweight progress HUD
  pod 'SDWebImage', '~> 4.4' #Asynchronous image downloader with cache support

  target 'GitHub StarsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GitHub StarsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
